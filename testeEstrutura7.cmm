int i;

void main() {
    i = 0;

    do {
        i = i + 1;
        if (i == 2) continue;
        if (i == 4) break;
        write("do i: ", i);
    } while (i < 5);

    i = 0;
    for (i = 0; i < 5; i = i + 1) {
        if (i == 1) continue;
        if (i == 3) break;
        write("for i: ", i);
    }
}
